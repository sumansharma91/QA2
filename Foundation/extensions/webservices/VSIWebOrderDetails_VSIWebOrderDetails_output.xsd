                                    </xsd:simpleType>
                                    </xsd:union>
                                    </xsd:simpleType>
                                    </xsd:attribute>

                                    <xsd:attribute
                                    name="TriggerAttributeValue"
                                    type="yfctype:Text-200" use="optional">

                                    <xsd:annotation
                                    yfc:columnName="ATTRIBUTE_VALUE" yfc:tableName="YPM_PRICING_RULE_ATTR_VALUE">
